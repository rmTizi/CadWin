                }

            /* Disabled */
                Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QPushButton:disabled,
                QSlider::handle:disabled,
                _ {
                    background-color: #626262;
                }
            
            /* Input Active */
                SpreadsheetGui--SheetTableView > QWidget > QAbstractSpinBox[readOnly="false"]:focus,
                SpreadsheetGui--SheetTableView > QWidget > QComboBox:on,
                SpreadsheetGui--SheetTableView > QWidget > QLineEdit[readOnly="false"]:focus,
                SpreadsheetGui--SheetTableView > QWidget > QPlainTextEdit[readOnly="false"]:focus,
                _{
                    background-color: #262626;
