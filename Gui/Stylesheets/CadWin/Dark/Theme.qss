                    ); 
                } */

        /* Text Control */
            /* Border */
                QLineEdit[readOnly="false"],
                QPlainTextEdit[readOnly="false"],
                QTextEdit[readOnly="false"],
                _ {
                    border-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop: 0.9998 rgba(255, 255, 255, 0.08),
                        stop: 1 rgba(255, 255, 255, 0.5442),
                        stop: 1 rgba(255, 255, 255, 0.5442)
                    );
                    border-top-color: rgba(255, 255, 255, 0.08);
                    border-bottom-color: rgba(255, 255, 255, 0.5442);
                }

    /* Stroke Color */
        /* Control Stroke */
            /* Default */
                QLineEdit[readOnly="false"]:pressed,
                QLineEdit:disabled,
                QPlainTextEdit[readOnly="false"]:pressed,
                QPlainTextEdit:disabled,
                QTextEdit[readOnly="false"]:pressed,
                QTextEdit:disabled,
                QAbstractSpinBox[readOnly="false"]:pressed,
                QAbstractSpinBox:disabled,
                QComboBox:pressed,
                QComboBox:disabled,
                QPushButton:pressed,
                QPushButton:disabled,
                _ {
                    border-color: rgba(255, 255, 255, 0.0698);
                }

