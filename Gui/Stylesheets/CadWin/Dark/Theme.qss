    /* Elevation */
        /* Control */
            /* Border */
                QLineEdit,
                QPlainTextEdit,
                QTextEdit,
                QAbstractSpinBox,
                QComboBox,
                QPushButton,
                _ { 
                    border-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop: 0 rgba(255, 255, 255, 0.093),
                        stop: 0.0957 rgba(255, 255, 255, 0.0698),
                        stop: 0.0958 rgba(255, 255, 255, 0.0698)
                    );                    
                    border-top-color: rgba(255, 255, 255, 0.093);
                    border-bottom-color: rgba(255, 255, 255, 0.0698);
                }

        /* Circle */
            /* Border */
                /* _ {
                    border-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop: 0 rgba(255, 255, 255, 0.093),
