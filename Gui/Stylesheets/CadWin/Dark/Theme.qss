/* Theme Color Styles */
    /* Fill Color */
        /* Text */
            /* Primary */
                * {
                    color: #FFFFFF;
                }

                SpreadsheetGui--SheetTableView,
                _ {
                    qproperty-foregroundColor: #FFFFFF;
                }

                SpreadsheetGui--SheetTableView,
                QComboBox QAbstractItemView,
                QListView,
                QTreeView,
                _ { 
                    selection-color : #FFFFFF;
                }

            /* Secondary */
                QTableView::item:hover,
                QStatusBar QLabel,
                QStatusBar,
                QSint--TaskHeader QSint--ActionLabel,
                QTabBar::tab:!hover:!selected,
                QMenuBar::item:pressed,
                QComboBox:pressed,
                QPushButton:pressed,
                _ {
                    color: rgba(255, 255, 255, 0.786);
                }
        
                Gui--PropertyEditor--PropertyEditor, 
                _ {
                    qproperty-groupTextColor: rgba(255, 255, 255, 0.786);
                }

            /* Tertiary */
                QTabBar::tab:pressed,
                _ { 
                    color : rgba(255, 255, 255, 0.5442);
                }

            /* Disabled */
                QTableView::item:disabled,
                QWidget:disabled,
                _ {
                    color: rgba(255, 255, 255, 0.3628);
                }
                
        /* Control */
            /* Transparent */
                QTableCornerButton::section,
                QHeaderView::section,
                * {
                    background-color: rgba(255, 255, 255, 0);
                }

            /* Default */
                QLineEdit,
                QPlainTextEdit,
                QTextEdit,
                QAbstractSpinBox,
                QComboBox, 
                QPushButton,
                _ {
                    background-color: rgba(255, 255, 255, 0.0605);
                }

            /* Secondary */
                QLineEdit[readOnly="false"]:hover,
                QPlainTextEdit[readOnly="false"]:hover,
                QTextEdit[readOnly="false"]:hover,
                QAbstractSpinBox[readOnly="false"]:hover,
                QComboBox:hover, 
                QPushButton:hover,
                _ {
                    background-color: rgba(255, 255, 255, 0.0837);
                }

            /* Tertiary */
                QLineEdit[readOnly="false"]:pressed,
                QPlainTextEdit[readOnly="false"]:pressed,
                QTextEdit[readOnly="false"]:pressed,
                QAbstractSpinBox[readOnly="false"]:pressed,
                QComboBox:pressed, 
                QPushButton:pressed,
                _ {
                    background-color: rgba(255, 255, 255, 0.0326);
                }

            /* Disabled */
                QLineEdit:disabled,
                QPlainTextEdit:disabled,
                QTextEdit:disabled,
                QAbstractSpinBox:disabled,
                QComboBox:disabled, 
                QPushButton:disabled,
                _ {
                    background-color: rgba(255, 255, 255, 0.0419);
                }

            /* Input Active */
                QLineEdit[readOnly="false"]:focus,
                QPlainTextEdit[readOnly="false"]:focus,
                QTextEdit[readOnly="false"]:focus,
                QAbstractSpinBox[readOnly="false"]:focus,
                QComboBox:on, 
                _ {
                    background-color: rgba(30, 30, 30, 0.7);
                }

        /* Control Strong */
            /* Default */
                QProgressBar,
                QWidget::handle,
                QSlider::add-page,
                _ {
                    background-color: rgba(255, 255, 255, 0.5442);
                }

            /* Disabled */
                /* _ {
                    background-color: rgba(255, 255, 255, 0.2465);
                } */

        /* Control Alt */
            /* Transparent */
                *::pane,
                *::tab,
                * {
                    border-color: rgba(255, 255, 255, 0);
                    gridline-color: rgba(40, 40, 40, 0);
                }

                QScrollBar::add-page,
                QScrollBar::sub-page,
                QScrollBar::add-line,
                QScrollBar::sub-line,
                QScrollBar,
                _ {
                    background-color: rgba(255, 255, 255, 0);
                }

                Gui--ExpLineEdit[readOnly="true"],
                Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QLabel:disabled,
                Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QLabel,
                QTabWidget#propertyTab QTabBar::tab,
                QToolBar > QToolButton:disabled,
                QToolBar > QToolButton,
                _ {
                    color: rgba(255, 255, 255, 0);
                }

            /* Secondary */
                QWidget::indicator,
                _ {
                    background-color: rgba(0, 0, 0, 0.1);
                }

            /* Tertiary */
                QWidget::indicator:hover,
                _ {
                    background-color: rgba(255, 255, 255, 0.0419);
                }

            /* Quaternary */
                QWidget::indicator:pressed, 
                _ {
                    background-color: rgba(255, 255, 255, 0.0698);
                }

            /* Disabled */
                QWidget::indicator:disabled, 
                _ {
                    background-color: rgba(255, 255, 255, 0);
                }

        /* Subtle */
            /* Transparent */
                QListView#listWidgetConstraints::indicator:checked,
                QListView#listWidgetConstraints::indicator:pressed,
                QListView#listWidgetConstraints::indicator:hover,
                QListView#listWidgetConstraints::indicator,
                QMenu::indicator:indeterminate,
                QMenu::indicator:checked,
                QMenu::indicator:pressed,
                QMenu::indicator:hover,
                QMenu::indicator,
                QTreeView::branch,
                QTreeView::item,
                QListView::item,
                QComboBox::drop-down,
                _ {
                    background-color: rgba(255, 255, 255, 0);
                }

                Gui--ExpLineEdit[readOnly="true"],
                QListView,
                QTreeView,
                _ { 
                    selection-background-color : rgba(255, 255, 255, 0);
                }

            /* Secondary */
                Gui--OverlayToolButton:hover,
                QTabWidget#propertyTab QTabBar::tab:hover,
                QTabWidget#propertyTab QTabBar::tab:selected,
                QToolBar#Workbench QTabBar::tab:!selected:hover,
                QToolBar > QToolButton:hover,
                QTreeView::branch:hover,
                QTreeView::branch:selected,
                QTreeView::item:hover,
                QTreeView::item:selected,
                QListView::item:hover,
                QListView::item:selected,
                QMenuBar::item:selected,
                QMenu::item:selected,
                QAbstractSpinBox::down-button:hover,
                QAbstractSpinBox::up-button:hover,
                QComboBox::drop-down:hover,
                _ {
                    background-color: rgba(255, 255, 255, 0.0605);
                }

                SpreadsheetGui--SheetTableView,
                QComboBox QAbstractItemView,
                _ { 
                    selection-background-color : rgba(255, 255, 255, 0.0605);
                }

            /* Tertiary */
                Gui--OverlayToolButton:pressed,
                QTabWidget#propertyTab QTabBar::tab:pressed,
                QAbstractSpinBox::up-button:pressed,
                QAbstractSpinBox::down-button:pressed,
                QToolBar > QToolButton:pressed,
                QMenuBar::item:pressed,
                QMenu::item:pressed,
                QComboBox::drop-down:pressed,
                _ {
                    background-color: rgba(255, 255, 255, 0.0419);
                }

            /* Disabled */
                QMenuBar::item:disabled,
                QMenu::item:disabled,
                _ {
                    background-color: rgba(255, 255, 255, 0);
                }

        /* Control Solid */
            /* Default */
                QSlider::handle,
                _ {
                    border-color: #454545;
                }

                SpreadsheetGui--SheetTableView > QWidget > QComboBox:focus,
                Gui--PropertyEditor--PropertyEditor > QWidget > QWidget > QPushButton,
