                QComboBox::drop-down:pressed,
                _ {
                    background-color: rgba(0, 0, 0, 0.0241);
                }

            /* Disabled */
                QMenuBar::item:disabled,
                QMenu::item:disabled,
                _ {
                    background-color: rgba(255, 255, 255, 0);
                }

