/* Theme Color Styles */
    /* Fill Color */
        /* Text */
            /* Primary */
                * {
                    color: rgba(0, 0, 0, 0.8956);
                }

                SpreadsheetGui--SheetTableView,
                _ {
                    qproperty-foregroundColor: rgba(0, 0, 0, 0.8956);
                }

                SpreadsheetGui--SheetTableView,
                QComboBox QAbstractItemView,
                QListView,
                QTreeView,
                _ { 
                    selection-color : rgba(0, 0, 0, 0.8956);
                }

            /* Secondary */
                QTableView::item:hover,
                QStatusBar QLabel,
                QStatusBar,
                QSint--TaskHeader QSint--ActionLabel,
                QTabBar::tab:!hover:!selected,
                QMenuBar::item:pressed,
                QComboBox:pressed,
                QPushButton:pressed,
                _ {
                    color: rgba(0, 0, 0, 0.6063);
                }
        
                Gui--PropertyEditor--PropertyEditor, 
                _ {
                    qproperty-groupTextColor: rgba(0, 0, 0, 0.6063);
                }

            /* Tertiary */
                QTabBar::tab:pressed,
                _ { 
                    color : rgba(0, 0, 0, 0.4458);
                }

            /* Disabled */
                QTableView::item:disabled,
                QWidget:disabled,
                _ {
                    color: rgba(0, 0, 0, 0.3614);
                }
                
        /* Control */
            /* Transparent */
                QTableCornerButton::section,
                QHeaderView::section,
                * {
                    background-color: rgba(255, 255, 255, 0);
                }

            /* Default */
                QLineEdit,
                QPlainTextEdit,
                QTextEdit,
                QAbstractSpinBox,
                QComboBox, 
                QPushButton,
                _ {
                    background-color: rgba(255, 255, 255, 0.7);
                }

            /* Secondary */
                QLineEdit[readOnly="false"]:hover,
                QPlainTextEdit[readOnly="false"]:hover,
                QTextEdit[readOnly="false"]:hover,
                QAbstractSpinBox[readOnly="false"]:hover,
                QComboBox:hover, 
                QPushButton:hover,
                _ {
                    background-color: rgba(249, 249, 249, 0.5);
                }

            /* Tertiary */
                QLineEdit[readOnly="false"]:pressed,
                QPlainTextEdit[readOnly="false"]:pressed,
                QTextEdit[readOnly="false"]:pressed,
                QAbstractSpinBox[readOnly="false"]:pressed,
                QComboBox:pressed, 
                QPushButton:pressed,
                _ {
                    background-color: rgba(249, 249, 249, 0.3);
                }

            /* Disabled */
                QLineEdit:disabled,
                QPlainTextEdit:disabled,
                QTextEdit:disabled,
                QAbstractSpinBox:disabled,
                QComboBox:disabled, 
                QPushButton:disabled,
                _ {
                    background-color: rgba(249, 249, 249, 0.3);
                }

            /* Input Active */
                QLineEdit[readOnly="false"]:focus,
                QPlainTextEdit[readOnly="false"]:focus,
                QTextEdit[readOnly="false"]:focus,
                QAbstractSpinBox[readOnly="false"]:focus,
                QComboBox:on, 
                _ {
                    background-color: #FFFFFF;
                }

        /* Control Strong */
            /* Default */
                QProgressBar,
                QWidget::handle,
                QSlider::add-page,
                _ {
                    background-color: rgba(0, 0, 0, 0.4458);
                }

            /* Disabled */
                /* _ {
                    background-color: rgba(0, 0, 0, 0.3173);
                } */

        /* Control Alt */
            /* Transparent */
                *::pane,
