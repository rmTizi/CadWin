        /* Text Control */
            /* Border */
                QLineEdit[readOnly="false"],
                QPlainTextEdit[readOnly="false"],
                QTextEdit[readOnly="false"],
                _ {
                    border-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop: 0.9999 rgba(0, 0, 0, 0.0578),
                        stop: 1 rgba(0, 0, 0, 0.4458)

                    );
                    border-top-color: rgba(0, 0, 0, 0.0578);
                    border-bottom-color: rgba(0, 0, 0, 0.4458);
                }

