    /* Elevation */
        /* Control */
            /* Border */
                QLineEdit,
                QPlainTextEdit,
                QTextEdit,
                QAbstractSpinBox,
                QComboBox,
                QPushButton,
                _ { 
                    border-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                        stop: 0.0958 rgba(255, 255, 255, 0.0698),
                        stop: 1.0 rgba(0, 0, 0, 0.1622)

                    );                    
                    border-top-color: rgba(0, 0, 0, 0.0578);
                    border-bottom-color: rgba(0, 0, 0, 0.1622);
                }

